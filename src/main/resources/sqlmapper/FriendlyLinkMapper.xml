    <update id="update" parameterType="org.jgenerator.model.FriendlyLink">
		update FriendlyLink 
        <set>
					<if test="id !=null">
						id = #{id},
					</if> 
					<if test="name !=null">
						name = #{name},
					</if> 
					<if test="url !=null">
