    <insert id="insert" parameterType="org.jgenerator.model.FriendlyLink" useGeneratedKeys="true" keyProperty="id">
		insert into FriendlyLink 
		(id, name, url, sort, attribute1, attribute2, attribute3)
		values
		(#{id}, #{name}, #{url}, #{sort}, #{attribute1}, #{attribute2}, #{attribute3})
	</insert>
	
	<select id="queryByCondition" parameterType="map" resultType="org.jgenerator.model.FriendlyLink">
		select * from FriendlyLink 
        <where>
            <include refid="condition"/>
            <include refid="andLikeCondition"/>
            <include refid="startLikeCondition"/>
        </where>
        <if test="orderBy==null and sortBy==null">
			order by sort
		</if>
        <if test="orderBy!=null">
			order by ${orderBy}
		</if>
        <if test="orderBy!=null and sortBy!=null">
			${sortBy}
		</if>
        <if test="beginRow==null or pageSize==null">
			limit 0,10
		</if>
        <if test="beginRow!=null and pageSize!=null">
			limit #{beginRow},#{pageSize}
		</if>
    </select>
    
    <select id="findAll" parameterType="map" resultType="org.jgenerator.model.FriendlyLink">
		select * from FriendlyLink 
        <where>
            <include refid="condition"/>
            <include refid="andLikeCondition"/>
        </where>
    </select>

    <select id="distinct" parameterType="map" resultType="org.jgenerator.model.FriendlyLink">
